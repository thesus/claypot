# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.py]
indent_style = space
indent_size = 4

[*.{js,vue,html}]
indent_style = space
indent_size = 2

[{package.json,.gitlab-ci.yml,docker-compose.yml}]
indent_style = space
indent_size = 2
